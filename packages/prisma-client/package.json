{
  "name": "@rooms2watch/prisma-client",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "migrate": "prisma migrate dev --name",
    "reset": "prisma migrate reset --force",
    "deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "pull": "prisma db pull",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc",
    "dev": "yarn clean && tsc-watch --noClear",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "prisma": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@rooms2watch/tsconfig": "*",
    "@rooms2watch/shared-lib": "*",
    "@rooms2watch/eslint-config-custom": "*",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  }
}
