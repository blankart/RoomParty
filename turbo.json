{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev:build": {
      "dependsOn": ["^dev:build", "build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "generate": {
      "outputs": ["node_modules/.prisma/client/**"],
      "inputs": ["prisma/schema.prisma"]
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "reset": {
      "cache": false
    },
    "deploy": {
      "cache": false
    },
    "pull": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env.local",
    "$PORT",
    "$SERVER_ENV",
    "$SERVER_PORT",
    "$NEXT_PUBLIC_SERVER_URL",
    "$SERVER_URL",
    "$DATABASE_URL",
    "$NEXT_PUBLIC_WEBSOCKET_URL",
    "$WEB_BASE_URL",
    "$NEXT_PUBLIC_WEB_BASE_URL",
    "$GOOGLE_OAUTH_CLIENT_ID",
    "$NEXT_PUBLIC_GOOGLE_OAUTH_CLIENT_ID",
    "$GOOGLE_OAUTH_CLIENT_SECRET",
    "$NEXT_PUBLIC_GOOGLE_OAUTH_CLIENT_SECRET",
    "$GOOGLE_WEB_OAUTH_CLIENT_ID",
    "$NEXT_PUBLIC_GOOGLE_WEB_OAUTH_CLIENT_ID",
    "$GOOGLE_WEB_OAUTH_CLIENT_SECRET",
    "$NEXT_PUBLIC_GOOGLE_WEB_OAUTH_CLIENT_SECRET",
    "$SERVER_SESSION_SECRET",
    "$SERVER_JWT_SECRET"
  ]
}
